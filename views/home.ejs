        <% if (head.subject=="chemistry") { %>
          <a href="/user/chemistry">
            <div class="p-4 transform scale-95 transition-all hover:scale-100 duration-100 cursor-pointer">
              <div class="h-full border-2 w-72 shadow-2xl border-gray-200 border-opacity-60 rounded-lg overflow-hidden">
                  <img class="lg:h-48 md:h-36 w-72 object-cover object-center " src="./chemistry.jpg" alt="blog">
                  <div class="p-6">
                      <h2 class="tracking-widest text-xs title-font font-medium text-gray-400 mb-1">SUBJECT</h2>
                      <h1 class="title-font text-lg font-medium text-gray-900 mb-3">The chemistry paper </h1>
                      <!-- <p class="leading-relaxed mb-3">Photo booth fam kinfolk cold-pressed sriracha leggings jianbing microdosing tousled waistcoat.</p> -->
                  
